<elements>
    <!-- 报价测试-->
    <!--iframe-->
    <element xpath = "//*[@id='tab-box']/div[2]/div[2]/div/iframe">quoteTestIframe</element>
    <!--费用类型-->
    <element xpath = "//label[text()='费用类型']/following-sibling::span[1]/input[1]">feeType</element>
    <!--打开费用类型列表-->
    <element xpath = "//label[text()='费用类型']/parent::*//span[@class='combo-arrow']">openFeeType</element>>
    <!--中转费选项-->
    <element xpath = "//div[text()='收中转费']">feeTypeOption</element>
    <!--使用网点-->
    <element xpath = "//label[text()='使用网点']/following-sibling::span[1]/input[1]">useSite</element>
    <!--使用网点弹框首个匹配结果-->
    <element xpath = "//tr[@id='datagrid-row-r1-2-0']/td[@field='siteCode']">useResult</element>>
    <!--寄件网点-->
    <element xpath = "//label[text()='寄件网点']/following-sibling::span[1]/input[1]">fromSite</element>
    <!--寄件网点弹框首个匹配结果-->
    <element xpath = "//tr[@id='datagrid-row-r2-2-0']/td[@field='siteCode']">fromResult</element>>
    <!--派件网点-->
    <element xpath = "//label[text()='派件网点']/following-sibling::span[1]/input[1]">toSite</element>
    <!--派件网点弹框首个匹配结果-->
    <element xpath = "//tr[@id='datagrid-row-r3-2-0']/td[@field='siteCode']">toResult</element>>
    <!--服务方式-->
    <element xpath = "//label[text()='服务方式']/following-sibling::span[1]/input[1]">serviceType</element>
    <!--产品类型-->
    <element xpath = "//label[text()='产品类型']/following-sibling::span[1]/input[1]">productType</element>
    <!--计费类型-->
    <element xpath = "//label[text()='计费类型']/following-sibling::span[1]/input[1]">calculateType</element>
    <!--重量-->
    <element xpath = "//label[text()='　　重量']/following-sibling::input[1]">weight</element>
    <!--有效日期-->
    <element xpath = "//label[text()='有效日期']/following-sibling::span[1]/input[1]">date</element>
    <!--具体时间-->
    <element xpath = "//label[text()='有效日期']/following-sibling::span[2]/input[1]">time</element>
    <!--计算按钮-->
    <element xpath = "//span[text()='计算']">calculateButton</element>
    <!--报价显示-->
    <element id = "fee">fee</element>

</elements>
